<raftkeeper>
    <logger>
        <level>trace</level>
        <log>/var/log/raftkeeper-server/log.log</log>
        <errorlog>/var/log/raftkeeper-server/log.err.log</errorlog>
        <size>1000M</size>
        <count>10</count>
        <stderr>/var/log/raftkeeper-server/stderr.log</stderr>
        <stdout>/var/log/raftkeeper-server/stdout.log</stdout>
    </logger>
    <keeper>
        <raft_settings>
            <election_timeout_lower_bound_ms>3000</election_timeout_lower_bound_ms>
            <election_timeout_upper_bound_ms>5000</election_timeout_upper_bound_ms>
        </raft_settings>
    </keeper>
</raftkeeper>
